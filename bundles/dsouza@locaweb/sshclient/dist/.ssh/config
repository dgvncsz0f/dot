TCPKeepAlive yes
ServerAliveInterval 30
Compression yes
CompressionLevel 9

Host *
  ForwardAgent yes
  HashKnownHosts no
  GSSAPIAuthentication no

Host github.com
  StrictHostKeyChecking no

Host *.u
  ProxyCommand /bin/sh -c '/usr/bin/nc -xnibbler0001.locaweb.com.br:1080 -Xconnect $(sign.py $(echo %h | sed s/\.u$//) %p) %p'
