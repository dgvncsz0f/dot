TCPKeepAlive yes
ServerAliveInterval 30
Compression yes
CompressionLevel 9

Host *
  ForwardAgent yes
  HashKnownHosts no
  GSSAPIAuthentication no

Host github.com
  StrictHostKeyChecking no

Host *.u
  ProxyCommand /bin/sh -c '/bin/nc -x127.0.0.1:1080 -X5 $(echo %h | sed s/\.u$//) %p'

Host mephisto
  DynamicForward 127.0.0.1:1080
  ForwardX11Trusted yes
