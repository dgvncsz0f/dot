Host *
  Compression yes
  TCPKeepAlive yes
  ForwardAgent yes
  HashKnownHosts no
  ServerAliveInterval 300
  GSSAPIAuthentication no

Host gateway.*.xerpa.com.br
  SendEnv AWS_*
  DynamicForward 127.0.0.1:1080

Host *.u
  ProxyCommand socat SOCKS4a:127.0.0.1:$(echo -n %h | sed s/.u$//):%p,socksport=1080 STDIO
